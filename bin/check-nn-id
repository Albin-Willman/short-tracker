#!/usr/bin/env ruby

require 'json'

file_path = File.join(File.dirname(__FILE__), "../dist/api/data.json")

data = JSON.parse(File.read(file_path))

nn_ids = {}
data['companies'].each do |key, val|
  nn_id = val['nn_id']
  if nn_id == '-'
    puts "skipping #{key}"
    next
  end
  if nn_ids[nn_id]
    puts "id: #{nn_id} taken by #{nn_ids[nn_id]} and again by #{key}"
  else
    nn_ids[nn_id] = key
  end
end
